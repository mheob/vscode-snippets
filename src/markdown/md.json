{
	"Markdown ToDo list item": {
		"body": ["- [${2:x}] ${1:enter ToDo}$0"],
		"description": "Markdown ToDo list item",
		"prefix": "mdTodo"
	},
	"Markdown code block": {
		"body": ["```${1:enter type}", "${2:enter code}", "```$0"],
		"description": "Markdown code block",
		"prefix": "mdCode"
	},
	"Markdown image": {
		"body": ["![${1:alt}](${2:path})$0"],
		"description": "Markdown image",
		"prefix": "mdImage"
	},
	"Markdown image with title": {
		"body": ["![${1:alt}](${2:path} \"${3:title}\")$0"],
		"description": "Markdown image with title",
		"prefix": "mdImageWithTitle"
	},
	"Markdown link": {
		"body": ["[${1:link text}](${2:url})$0"],
		"description": "Markdown link",
		"prefix": "mdLink"
	},
	"Markdown link with title": {
		"body": ["[${1:link text}](${2:url} \"${3:title}\")$0"],
		"description": "Markdown link with title",
		"prefix": "mdLinkWithTitle"
	},
	"Markdown linked image": {
		"body": ["[![${1:alt}](${2:path})](${3:url})$0"],
		"description": "Markdown linked image",
		"prefix": "mdLinkedImage"
	},
	"Markdown linked image with title": {
		"body": ["[![${1:alt}](${2:path} \"${3:title}\")](${4:url})$0"],
		"description": "Markdown linked image with title",
		"prefix": "mdLinkedImageWithTitle"
	}
}
