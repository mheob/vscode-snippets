{
	"Arrow Function": {
		"body": ["const ${1:functionName} = (${2:variable}) => $0"],
		"description": "Arrow Function",
		"prefix": "arrowFunction"
	},
	"Arrow Function Inline": {
		"body": ["(${1:variable}) => $0"],
		"description": "Arrow Function Inline",
		"prefix": "arrowFunctionInline"
	},
	"Set Interval Function": {
		"body": ["setInterval(() => {", "\t$0", "}, ${1:intervalInMs})", ""],
		"description": "Executes the given function at specified intervals",
		"prefix": "setInterval"
	},
	"Set Timeout Function": {
		"body": ["setTimeout(() => {", "\t$0", "}, ${1:delayInMs})", ""],
		"description": "Executes the given function after the specified delay",
		"prefix": "setTimeout"
	}
}
