{
	"Export * from": {
		"body": ["export * from '${1:module}'$0"],
		"description": "Export * from",
		"prefix": "expAllFrom"
	},
	"Export As": {
		"body": ["export { ${2:originalName} as ${3:alias} } from '${1:module}'$0"],
		"description": "Export As",
		"prefix": "expAs"
	},
	"Export Const": {
		"body": ["export const ${1:name} = ${2:value}$0"],
		"description": "Export Const",
		"prefix": "expConst"
	},
	"Export Default": {
		"body": ["export default $0"],
		"description": "Export Default",
		"prefix": "expDefault"
	},
	"Import": {
		"body": "import ${2} from '${1:module}'$0",
		"description": "Import",
		"prefix": "impFrom"
	},
	"Import * from": {
		"body": ["import { ${2:originalName} as ${3:alias} } from '${1:module}'$0"],
		"description": "Import * from",
		"prefix": "impAllFrom"
	},
	"Import Destructured": {
		"body": "import { ${2} } from '${1:module}'$0",
		"description": "Import Destructured",
		"prefix": "impDestructured"
	},
	"Import File Only": {
		"body": "import '${1:module}'$0",
		"description": "Import File Only",
		"prefix": "impFileOnly"
	}
}
