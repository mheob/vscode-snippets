{
	"publisher": "mheob",
	"name": "vscode-snippets",
	"displayName": "Snippets for Next.js, React in TypeScript",
	"version": "2.1.1",
	"private": true,
	"description": "Snippets for the development with Next.js, React in a TypeScript environment.",
	"categories": [
		"Snippets"
	],
	"keywords": [
		"next.js",
		"javascript",
		"react",
		"snippets",
		"typescript"
	],
	"icon": "images/logo.png",
	"homepage": "https://github.com/mheob/vscode-snippets#readme",
	"bugs": {
		"url": "https://github.com/mheob/vscode-snippets/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mheob/vscode-snippets.git"
	},
	"license": "MIT",
	"type": "module",
	"files": [
		"LICENSE",
		"README.md",
		"images",
		"package.json",
		"snippets"
	],
	"scripts": {
		"build": "pnpm clean && tsx scripts/generate-snippet-files.ts",
		"clean": "rimraf snippets",
		"deploy": "pnpm exec vsce package --no-dependencies",
		"lint": "eslint .",
		"prepare": "husky install",
		"vscode:prepublish": "pnpm build"
	},
	"contributes": {
		"snippets": [
			{
				"language": "markdown",
				"path": "snippets/markdown.json"
			},
			{
				"language": "javascript",
				"path": "snippets/javascript.json"
			},
			{
				"language": "javascriptreact",
				"path": "snippets/javascriptreact.json"
			},
			{
				"language": "typescript",
				"path": "snippets/typescript.json"
			},
			{
				"language": "typescriptreact",
				"path": "snippets/typescriptreact.json"
			}
		]
	},
	"lint-staged": {
		"**/*.{ts,json,md,yml}": "prettier --write",
		"**/*.ts": "eslint --fix"
	},
	"devDependencies": {
		"@mheob/eslint-config": "^9.1.0",
		"@mheob/prettier-config": "^3.5.0",
		"@types/node": "^24.11.0",
		"@typescript-eslint/eslint-plugin": "^8.56.1",
		"@typescript-eslint/parser": "^8.56.1",
		"@vscode/vsce": "^3.7.1",
		"eslint": "^9.39.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.3.1",
		"prettier": "^3.8.1",
		"rimraf": "^6.1.3",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@10.27.0",
	"engines": {
		"vscode": "^1.102.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@vscode/vsce-sign",
			"esbuild",
			"keytar",
			"unrs-resolver"
		]
	},
	"sponsor": {
		"url": "https://github.com/sponsors/mheob"
	}
}
